#!/usr/bin/env sh

bindir=$(dirname $0)
etcdir=$bindir/../etc

. $bindir/installation.conf


java $JAVA_OPTS \
  -Djs.client.dir=$js_client_dir \
  -Dcss.dir=$css_dir \
  -Dlocal.maven.repo.path=$m2_repo_dir \
  -jar $jboss_modules_jar \
  -modulepath $modulepath \
  org.projectodd.restafari.bootstrap:main $etcdir/mboss-config.json $*
